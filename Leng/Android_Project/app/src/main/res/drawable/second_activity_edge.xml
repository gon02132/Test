<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp">

        <!--사각형 테두리-->
        <shape android:shape="rectangle">

            <!--코너-->
            <corners android:radius="5dp"/>

            <!--테두리 두께와 색상 지정-->
            <stroke
                android:width="2dp"
                android:color="#565656" />
            <!--배경 투명도+색상 투명도는 앞2자리로 16진수로 표현-->
            <solid android:color="#9FFFFFFF" />

        </shape>

    </item>

</selector>